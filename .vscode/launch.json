{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceFolder}/02.Kernel64/Temp/Kernel64.elf",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "target": "localhost:1234",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "autorun": [
                "set architecture i386:x86-64"
            ]
        }
    ]
}